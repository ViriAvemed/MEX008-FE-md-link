const fs = require ("fs");
const typeOfFile = require('./restrictFile');

// const fs = require ("fs");

const readFileMd = () => {
  if(typeOfFile()){

    fs.readFile("./README.md", "utf8", (error , data) => {
        if (error) {
          console.error(error);
        }else {
          console.log(data);

        }
    })

      }else {
        console.log("archivo inv√°lido");
      }
};

readFileMd();
module.exports = readFileMd;




